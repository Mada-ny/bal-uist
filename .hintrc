{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-filter",
          "-webkit-text-size-adjust",
          "-webkit-text-decoration-skip",
          "-ms-touch-action",
          "-webkit-tap-highlight-color",
          "-webkit-overflow-scrolling",
          "min-height: auto"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ]
  }
}